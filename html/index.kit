<body data-bs-spy="scroll" data-bs-target="#navbar">

    <!--@include 'nav.kit'-->
    <section id="home">
        <div class="hero-img">
            <div class="hero-shadow"></div>
            <div class="hero-text focus-in-expand-fwd">
                <h1><span>Dog</span>ger</h1>
                <p>Profesjonalne szkolenia psów</p>
            </div>
        </div>
    </section>

    <section id="aboutus" class="aboutus">
        <div class="wrapper">
            <h1 class="title">O nas</h1>
            <div class="aboutus-box">
                <img class="about-img" src="./dist/img/dama1.jpg" alt="Szczęśliwy pies na niebieskim tle">

                <div class="aboutus-text" data-aos="fade-right">
                    <h3>Psy to nasza pasja</h3>
                    <p>Lorem ipsum, dolor sit amet consectetur adipisicing elit. Architecto nulla praesentium quod
                        ducimus
